    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^26.0.24",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "css-modules-typescript-loader": "^4.0.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-typescript-sort-keys": "^1.7.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^2.1.0",
    "mini-svg-data-uri": "^1.3.3",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "process": "^0.11.10",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "build": "webpack --config webpack.production.js --bail --mode production",
    "ci:lint:code": "eslint --ext .ts,.tsx --max-warnings 0 src/ tests/",
    "ci:test:integration": "yarn run test:integration --ci --colors",
    "ci:test:unit": "yarn run test:unit --ci --colors",
    "lint": "eslint --ext .ts,.tsx --max-warnings 0 src/ tests/ --cache",
    "presetup": "yarn add --dev chalk inquirer @types/inquirer ts-node @types/node clui @types/clui figlet @types/figlet",
    "setup": "ts-node ./internals/index.ts",
    "start": "npm-run-all --parallel start:*",
    "start:web": "webpack serve --config webpack.local.js --mode development",
    "test": "npm-run-all --sequential lint test:*",
    "test:integration": "jest tests/",
    "test:types": "tsc",
    "test:unit": "jest src/ --coverage"
  }
}
